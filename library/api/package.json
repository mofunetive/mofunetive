{
	"private": false,
	"name": "@mofunetive/api",
	"version": "1.0.0",
	"license": "MPL-2.0",
	"author": "Faelayis <48393914+Faelayis@users.noreply.github.com>",
	"repository": {
		"type": "git",
		"url": "https://github.com/mofunetive/mofunetive.git",
		"directory": "library/api"
	},
	"type": "module",
	"exports": {
		".": {
			"import": "./lib-esm/library.js",
			"require": "./dist/library.js"
		}
	},
	"module": "lib-esm/library.js",
	"files": [
		"dist",
		"lib",
		"lib-esm"
	],
	"scripts": {
		"build-esm": "tsc -m es6 --outDir lib-esm",
		"build-ts": "tsc -p .",
		"build-webpack": "NODE_OPTIONS=--loader=ts-node/esm webpack --config webpack.config.ts",
		"ci:bump": "bump",
		"ci:delete_scripts": "pnpm pkg delete scripts",
		"clean": "rimraf dist lib lib-esm",
		"dev": "tsc -w",
		"format": "prettier --write .",
		"postinstall": "pnpm build-ts && pnpm build-esm",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix"
	},
	"typings": "lib-esm/library.d.ts",
	"dependencies": {
		"@mofunetive/metadata": "1.1.0",
		"@octokit/graphql-schema": "13.2.0",
		"@octokit/openapi-types": "16.0.0",
		"@octokit/rest": "19.0.7",
		"@octokit/types": "9.0.0",
		"axios": "1.3.2",
		"discord-api-types": "0.37.33"
	},
	"devDependencies": {
		"@babel/core": "7.20.12",
		"@graphql-eslint/eslint-plugin": "3.15.0",
		"@types/node": "18.13.0",
		"@types/webpack": "5.28.0",
		"clean-webpack-plugin": "4.0.0",
		"cross-env": "7.0.3",
		"graphql": "16.6.0",
		"prettier": "2.8.3",
		"terser-webpack-plugin": "5.3.6",
		"ts-loader": "9.4.2",
		"ts-node": "10.9.1",
		"tslib": "2.5.0",
		"typescript": "4.9.5",
		"typescript-transform-paths": "3.4.6",
		"typescript-webpack-plugin": "^0.0.5",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.1"
	},
	"publishConfig": {
		"access": "public",
		"linkDirectory": false
	},
	"svelte": "lib-esm/library.js"
}
