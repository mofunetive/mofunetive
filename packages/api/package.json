{
	"name": "@mofunetive/api",
	"version": "1.0.1",
	"private": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/mofunetive/mofunetive.git",
		"directory": "packages/api"
	},
	"license": "MPL-2.0",
	"author": "Faelayis <48393914+Faelayis@users.noreply.github.com>",
	"type": "module",
	"exports": {
		".": {
			"import": "./esm/library.js",
			"require": "./dist/library.js"
		}
	},
	"svelte": "esm/library.js",
	"module": "esm/library.js",
	"typings": "esm/library.d.ts",
	"files": [
		"dist",
		"esm"
	],
	"scripts": {
		"build": "bun esbuild.ts && tsc --emitDeclarationOnly --outDir esm",
		"ci:bump": "bump",
		"clean": "bunx rimraf .turbo dist esm tsconfig.tsbuildinfo node_modules",
		"dev": "tsc --watch",
		"format": "prettier --write .",
		"postinstall": "bun run build",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix"
	},
	"dependencies": {
		"@mofunetive/metadata": "1.1.1",
		"@octokit/graphql-schema": "15.26.0",
		"@octokit/openapi-types": "24.2.0",
		"@octokit/rest": "21.1.1",
		"@octokit/types": "13.10.0",
		"axios": "1.5.0",
		"discord-api-types": "0.37.57"
	},
	"devDependencies": {
		"@babel/core": "7.22.20",
		"@graphql-eslint/eslint-plugin": "4.3.0",
		"@types/node": "22.13.11",
		"cross-env": "7.0.3",
		"graphql": "16.8.1",
		"ts-loader": "9.4.4",
		"ts-node": "10.9.1",
		"tslib": "2.6.2",
		"typescript": "5.2.2"
	},
	"publishConfig": {
		"access": "public",
		"linkDirectory": false
	}
}
