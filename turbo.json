{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"outputs": ["lib-esm/**"],
			"cache": true
		},
		"check-types": {
			"dependsOn": ["^check-types"]
		},
		"dev": {
			"persistent": true,
			"cache": false
		},
		"clean": {
			"cache": false,
			"persistent": true
		}
	}
}
